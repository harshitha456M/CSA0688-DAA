#include<stdio.h>
#include<string.h>
int main()
{
	char a[20];
	int i,length,b=0;
	printf("enter a word or number:");
	scanf("%s",a);
	length=strlen(a);
	for(i=0;i<length;i++)
	{
		if(a[i]==a[length-i-1])
		b++;
	}
		if(b==i)
		{
			printf(" pallendrome");
		}
		else
		{
			printf(" not  pallendrome");
		}
	
	
}
