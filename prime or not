#include<stdio.h>
int main()
{
	int n,i,c;
	printf("enter number:");
	scanf("%d",&n);
	if(n<=0)
	{
		printf("invalid");
	}
	else if(n==1)
	{
		printf(" 1 is neither prime or composite");
	}
	else
	{
	for(i=2;i<=n;i++)
	{
		if(n%i==0)
		c++;
	}
	if(c>=2)
	printf("prime");
	else
	printf("not prime");
}
	
}
